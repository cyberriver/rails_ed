<h1>All Tests</h1>
<table>
  <thead>
    <tr>
      <th>#test</th>
      <th>test title</th>
      <th>test level</th>
      <th>test category</th>
      <th>author_id</th>
      <th>Questions count</th>
    </tr>
  </thead>

  <tbody>
    <% @test.each do |test|%>
    <tr>

      <td><%=test.id %> </td>
      <td><%=test.title %> </td>
      <td><%=level_title(test)%> </td>
      <td><%=test.category_id%> </td>
      <td><%=test.author_id %> </td>
      <td><%=test.questions.count %></td>

      <td> <li><%=button_to "Start", start_test_path(test.id), method: :post %></li></td>

    </tr>
    <% end %>
  </tbody>

</table>
