<h1>Test Questions</h1>
<table>
  <thead>
    <tr>
      <th>#question</th>
      <th>question body</th>
      <th colspan="2"></th>
    </tr>
  </thead>
  <tr>
   Create new question
   <%= link_to "Create", new_test_question_path %>
  </tr>

  <tbody>
    <% @questions.each do |q|%>
    <tr>
      <td><%=q.id %> </td>
      <td><%=q.body %> </td>
      <td><%= link_to "Show", question_path(q.id) %></td>
      <td><%= link_to "Edit", edit_question_path(q.id) %></td>
      <td><%= button_to "Delete", question_path(q.id), method: :delete, class: "btn btn-danger", data: { confirm: "Are you sure?" } %></td>
    </tr>
    <% end %>
  </tbody>

</table>
