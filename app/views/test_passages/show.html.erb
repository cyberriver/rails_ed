<h1>Pass the <%= @test_pessage.test.title%>  Test </h1>

<b>
  <%= @test_passage.current_question.body%>
</b>

<%= form_with url: test_passage_path(@test_passage), local:true, method: :put do |form| %>
  <%= form.collection_check_boxes :aswer_ids, @test_passage.current_question.answers, :id, :body do |b| %>
  <p>
    <%= b.check_box id: "answer_ids_#{b.object.id}"%>
    <%= b.label%>
  </p>
  <% end%>
<% end %>
