
<table class="table">
  <thead>
    <tr>
      <th scope="col"><%=t('question')%></th>
      <th scope="col"><%=t('.gist_url')%></th>
      <th scope="col"><%=t('.email')%></th>
    </tr>
  </thead>

  <tbody>
    <% @gists.each do |gist| %>
    <tr>
      <td><%= link_to "#{gist.question.body.truncate(25)}",admin_question_path(gist.question) %></td>
      <td><%= link_to "#{gist.gist_url}", gist.gist_url%></td>
      <td><%= gist.email %></td>

    </tr>
    <% end %>
  </tbody>

</table>
