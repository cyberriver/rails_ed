
<table class="table">
  <thead>
    <tr>
      <th scope="col"><%=t('question')%></th>
      <th scope="col"><%=t('.gist_url')%></th>
      <th scope="col"><%=t('.email')%></th>
    </tr>
  </thead>

  <tbody>
    <% @gists.each do |gist| %>
    <tr>
      <td><%= gist.question.body %></td>
      <td><%= gist.gist_url %></td>
      <td><%= gist.email %></td>

    </tr>
    <% end %>
  </tbody>

</table>
