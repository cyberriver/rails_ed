
<%= form_with model: [:admin, test],
                     url: update_inline_admin_test_path(test),
                     data:{ test_id: test.id },
                     class: 'form-inline hide' do |f| %>

  <%= f.text_field :title, class: 'form-control mr-2' %><br />
  <%= f.text_field :category_id, class: 'form-control mr-2' %><br />
  <%= f.submit t('submit'), class: 'btn btn-primary'  %>

<% end %>
